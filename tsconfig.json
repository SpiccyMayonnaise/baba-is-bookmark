{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmit": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es5",
    "jsx": "react",
    "lib": [
      "DOM",
      "es6"
    ],
    "baseUrl": "src",
    "rootDir": "src"
  },
  "include": [
    "src/",
  ],
  "compileOnSave": false
}